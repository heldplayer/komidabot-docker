{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "non_changing",
      "version": 1,
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "1d",
        "strategy": "performance"
      },
      "urls": [
        "/campus"
      ]
    },
    {
      "name": "volatile",
      "version": 1,
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "1h",
        "strategy": "performance"
      },
      "urls": [
        "/campus/*/closing_days",
        "/campus/*/menu/*"
      ]
    }
  ]
}
