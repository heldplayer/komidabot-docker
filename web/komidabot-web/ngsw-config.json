{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/assets/i18n/**",
          "/assets/*.json"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "!/assets/i18n/**",
          "!/assets/*.json",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "volatile",
      "version": 1,
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "strategy": "performance"
      },
      "urls": [
        "/api/campus/closing_days/*",
        "/api/campus/*/closing_days/*",
        "/api/campus/*/menu/*",
        "/api-dev/campus/closing_days/*",
        "/api-dev/campus/*/closing_days/*",
        "/api-dev/campus/*/menu/*"
      ]
    },
    {
      "name": "non_changing",
      "version": 1,
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "7d",
        "strategy": "performance"
      },
      "urls": [
        "/api/campus"
      ]
    }
  ]
}
